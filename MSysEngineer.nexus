            "type": "function",
            "name": "onLoad",
            "enabled": true,
            "id": 2,
            "code": "if (MSys == undefined || MSys.coreVersion == undefined || MSys.coreVersion < 1.3) {
    display_notice('Starwraith Engineer Combat Implant v1.3 requires MSysCore v1.3 or later','gold');
    return;
}

//Fury
MSys.shortname = 'SECI'
MSys.parts = 0
MSys.repeat = true
MSys.wormDeployed = false
MSys.huntingAttack = function() {
    if (!MSys.systemEnabled) {return}
    var command = ''

    //Engineer
    if (MSys.health / MSys.maxHealth < .8 && MSys.healReady && MSys.useHeal) {
        MSys.debugMessage('Activating stimjector')
        send_command('operate stimjector')

    }else if (get_variable('tar') == '' || MSys.offenseEnabled == false) {
        return;

    }else if (!MSys.repeat && get_variable('useRepeat') && get_variable('repeatAttack') != '') {
        MSys.debugMessage('Initiating repeat')
        command =  'bot repeat '
        command += get_variable('engineerRepeatAttack')
        command += ' '

    } else if (get_variable('useWorm') && MSys.targetHealth > 0.7 && !MSys.wormDeployed) {
        MSys.debugMessage('Executing burrowbot worm attack')
        command = 'bot worm '

    } else if (get_variable('useRelease') && MSys.wormDeployed && MSys.targetHealth < 0.3) {
        MSys.debugMessage('Releasing burrowbot')
        command = 'bot release '
    } else {
        MSys.debugMessage('Executing default engineer attack')
        command = 'bot claw '
    }

    MSys.debugMessage('Attempting command: ' + command) 
    command += get_variable('tar')
    send_command(command)
}

MSys.loaded(\"Starwraith Engineer Combat Implant v1.3\");
send_command('wakeup')
