    "name": "msysnanoseer",
    "enabled": true,
    "id": 1,
    "items": [
        {
            "type": "function",
            "name": "onLoad",
            "enabled": true,
            "id": 2,
            "code": "if (MSys == undefined || MSys.coreVersion == undefined || MSys.coreVersion < 1.3) {
    display_notice('Starwraith Nanoseer Combat Implant v1.3 requires MSysCore v1.3 or later','gold');
    return;
}

//Nanoseer Character Information
MSys.shortname  = 'SNCI'
MSys.nanites    = 0
MSys.maxNanites = 0
MSys.sanity     = 0
MSys.maxSanity  = 0

//Configuration
MSys.useDelay           = get_variable('useDelay')
MSys.useFreeze          = get_variable('useFreeze')
MSys.useFrenzy          = get_variable('useFrenzy')
MSys.useMark            = get_variable('useMark')
MSys.useMiniaturization = get_variable('useMiniaturization')
MSys.useMultistrike     = get_variable('useMultistrike')
MSys.useNegation        = get_variable('useNegation')
MSys.usePointZero       = get_variable('usePointZero')
MSys.useProtection      = get_variable('useProtection')
MSys.useRush            = get_variable('useRush')
MSys.useSoothe          = get_variable('useSoothe')
MSys.useSpeedup         = get_variable('useSpeedup')

//Active Abilities
MSys.empyreal        = ''
MSys.delaySwarm      = false
MSys.freeze          = false
MSys.frenzy          = false
MSys.mark            = false
MSys.miniaturization = false
MSys.multistrike     = false
MSys.negation        = false
MSys.pointZero       = false
MSys.protection      = false
MSys.rush            = false
MSys.soothing        = false
MSys.sootheNeeded    = false
MSys.speedup         = false
MSys.speedupAvailable= true

MSys.huntingAttack = function() {
    if (!this.systemEnabled || MSys.escaping) {return}
    
    var command = ''
    
    if (MSys.interrupt && MSys.offenseEnabled) {
        this.debugMessage('Attempting eyestrike interrupt')
        send_command('nano eyestrike ' + get_variable('tar'))
        return

    // Do not interrupt channel
    } else if (this.soothing) {
        this.debugMessage('Channeling Soothe')
        return

    //Self heal
    } else if (this.healthPct < .8 && this.healReady && this.useHeal) {
        this.debugMessage('Nano repair')
        send_command('nano repair')
        return

    //Out of combat buffs
    //Casting and channeling are separated to enable the buff to be executed
    //immediately if already channeling one of the required empyreals
    //This prevents unnecessary swapping of empyreal channels after combat
    } else if (!this.rush && this.useRush) {
        this.debugMessage('Cast Rush')
        send_command('Rush')
        return

    } else if (!this.miniaturization && this.useMiniaturization && MSys.nanites >= 350) {
        this.debugMessage('Cast Miniaturization')
        send_command('nano miniaturisation')
        return

    } else if (!this.protection && this.useProtection && MSys.nanites >= 100) {
        this.debugMessage('Cast Protection')
        send_command('nano protect')
        return

    } else if (!this.negation && this.useNegation && this.empyreal == 'Progenitor') {
        this.debugMessage('Cast Negation')
        send_command('negation')
        return

    } else if (!this.frenzy && this.useFrenzy && this.empyreal == 'Conqueror') {
        this.debugMessage('Cast Frenzy')
        send_command('frenzy')
        return

    } else if (!this.frenzy && this.useFrenzy && this.empyreal != 'Conqueror') {
        this.debugMessage('Channel conqueror')
        send_command('oblivion channel conqueror')
        return

    } else if (!this.negation && this.useNegation && this.empyreal != 'Progenitor') {
        this.debugMessage('Channel Progenitor')
        send_command('oblivion channel progenitor')
        return

    } else if (this.sootheNeeded && !this.soothing) {
        this.debugMessage('Soothing needed after combat')
        send_command('void soothe')
        return

        //Abort execution of offensive actions if disabled
    } else if (!this.offenseEnabled || get_variable('tar') == '') {
        this.debugMessage('No target or offense disabled')
        return

        //Combat preparation
    } else if (!this.speedup && this.useSpeedup && MSys.speedupAvailable && this.empyreal != 'Traveller') {
        this.debugMessage('Channel Traveller')
        send_command('oblivion channel Traveller')
        return

    } else if (!this.speedup && this.useSpeedup && MSys.speedupAvailable && this.empyreal == 'Traveller') {
        this.debugMessage('Cast Speedup')
        send_command('oblivion speedup')
        return

    } else if (!this.mark && this.useMark && this.empyreal != 'Traveller') {
        this.debugMessage('Channel Traveller')
        send_command('oblivion channel Traveller')
        return

    } else if (!this.mark && this.useMark && this.empyreal == 'Traveller') {
        this.debugMessage('Cast Mark')
        send_command('oblivion mark')
        return

    } else if (!this.pointZero && this.usePointZero) {
        this.debugMessage('Point Zero')
        send_command('void pointzero')
        return

    } else if (!this.delaySwarm && this.useDelay) {
        this.debugMessage('Delay Attack')
        command =  'delay '
        command += get_variable('delayAttack')
        command += ' '

    } else if (!this.multistrike && this.targetHealth > 80 && this.useMultistrike) {
        this.debugMessage('Multistrike')
        command = 'multistrike '

    } else if (!this.freeze && this.useFreeze && this.targetHealth > 20 ) {
        this.debugMessage('Freeze')
        command = 'freeze '

    } else {
        this.debugMessage('Default Attack')
        command = this.attack + ' '
    }

    command += get_variable('tar')
    send_command(command)
}

MSys.loaded(\"Starwraith Nanoseer Combat Implant v1.3\");
send_command('wakeup');",
            "items": []
