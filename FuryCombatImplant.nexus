            "type": "function",
            "name": "onGMCP",
            "enabled": true,
            "id": 3,
            "code": "// Place any code here you'd like to run when a GMCP message is received
// - The GMCP message is received as \"args.gmcp_method\" and \"args.gmcp_args\"

GMCP[args.gmcp_method] = args.gmcp_args

/* ------------------------------------------ CHARACTER STUFF ----------------------------------- */

if (args.gmcp_method == \"Char.Vitals\") {

    // Track health changes
    var change = SFCI.health - args.gmcp_args.hp;
   
    if (change > 0) {
        SFCI.log('Damage taken: ' + change, 'yellow')
    } else if (change < 0) {
        SFCI.log('Damage healed: ' + Math.abs(change), '#6bdaee')
    }
    
    SFCI.health = args.gmcp_args.hp
    SFCI.maxHealth = args.gmcp_args.maxhp

    SFCI.stance = args.gmcp_args.st
    SFCI.rage = args.gmcp_args.rg
    SFCI.maxRage = args.gmcp_args.maxrg
}

/* ----------------------------------- DEFENCES AND BUFFS --------------------------------------- */

if (args.gmcp_method == 'Char.Defences.Add') {
    
    //Fury
    //if (args.gmcp_args.name == 'Oblivion: Conqueror')  {
    //    SFCI.empyreal = 'Conqueror'
    //}

}

/* -------------------------------------------- ITEMS IN ROOM ----------------------------------- */

//Get Items In Room
if (args.gmcp_method == \"Char.Items.List\") {
    SFCI.itemsInRoom = args.gmcp_args['items']
}

//Remove items when items leave
if (args.gmcp_method == \"Char.Items.Remove\") {
    let itemLeaving = args.gmcp_args['item']
    
    //Find index of item
    SFCI.itemsInRoom = SFCI.itemsInRoom.filter(item => item.id !== itemLeaving.id);
    
}

//Add items when items enter
if (args.gmcp_method == \"Char.Items.Add\") {
    let itemJoining = args.gmcp_args['item']
    
    //Find index of item
    SFCI.itemsInRoom.push(itemJoining);
    
    if (SFCI.systemEnabled === true) {
        if (!get_variable('tar')) {
            SFCI.findTarget()
        }
    }
    
    if (SFCI.infernoCharging && itemJoining.attrib === 'm') {
        send_command('asdf')
        SFCI.infernoCharging = false
        SFCI.alert('mob entered - inferno cancelled', 'white', 'red')
    }
}

/* -------------------------------------------- PLAYERS IN ROOM ----------------------------------- */
if (args.gmcp_method == \"Room.Players\") {
    SFCI.playersInRoom = args.gmcp_args
}

/* --------------------------------------------- HELPERS --------------------------------- */
if (args.gmcp_method == \"Room.Info\") {
    if (SFCI.systemEnabled === true) {
        SFCI.findTarget()
    }
}

if (args.gmcp_method == \"IRE.Target.Info\") {
    SFCI.targetHealth = args.gmcp_args['hpperc'].slice(0, -1)
}
